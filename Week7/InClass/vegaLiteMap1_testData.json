{
	"$schema":"https://vega.github.io/schema/vega-lite/v5.json",
	"width":500,
	"height":300,
	"layer":[
		{
			"data":{
				"url":"https://raw.githubusercontent.com/blackmad/neighborhoods/master/rome-rioni.geojson",
				"format":{
					"type":"topojson",
					"feature":"boroughs"
				}
			},
			"projection":{
				"scale":200000,
				"center":[
					12.5,
					41.862
				],
				"clipExtent":[
					[
						0,
						0
					],
					[
						400,
						300
					]
				]
			},
			"mark":{
				"type":"geoshape",
				"stroke":"white",
				"strokeWidth":2
			},
			"encoding":{
				"color":{
					"value":"#eee"
				}
			}
		},
		{
			"data":{
				"url":"https://raw.githubusercontent.com/pvac0001Monash/3179/main/Week7/InClass/testData1.csv"
			},
			"projection":{
				"scale":600000,
				"center":[
					12.5,
					41.862
				],
				"clipExtent":[
					[
						0,
						0
					],
					[
						400,
						300
					]
				]
			},
			"mark":"circle",
			"encoding":{
				"longitude":{
					"field":"long",
					"type":"quantitative"
				},
				"latitude":{
					"field":"lat",
					"type":"quantitative"
				},
				"size":{
					"value":100
				},
				"color":{
					"field":"id",
					"type":"nominal"
				}
			}
		}
	]
}
